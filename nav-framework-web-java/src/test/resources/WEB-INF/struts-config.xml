<struts-config>

	<!-- Used in UnitTest -->


	<!-- ======================================== Form Bean Definitions -->

	<form-beans>

		<form-bean name="frameworkErrorForm" type="no.nav.web.framework.action.FrameworkErrorForm"/>

		<form-bean name="secondForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="2name1" type="java.lang.String" />
