<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
	"http://www.springframework.org/dtd/spring-beans.dtd">


<beans>

	
	
	<bean id="brukeroversiktFormAction" class="org.springframework.webflow.action.FormAction">
		<property name="formObjectName" value="hentPersonForm"/>
		<property name="formObjectClass" value="no.nav.presentation.pensjon.saksbehandling.form.HentPersonForm"/>
	    <property name="formObjectScope" value="FLOW"/>
		
		<property name="validator">
			<ref bean="hentPersonValidator"/>
		</property>
	</bean>
	
	
	
	<bean id="hentPersonValidator" class="no.nav.presentation.pensjon.common.validation.MyValangValidator">
		<property name="messageSource" ref="messageSource" />

	 	<property name="customFunctions">
			<map>
				<entry key="modulus11">
					<value>no.nav.presentation.pensjon.saksbehandling.stelvio.validate.Modulus11Function</value>
				</entry>
			</map>
		</property>
		
	  <property name="valang">
	    <value>
	    <![CDATA[
	    	{ fodselsnummer : match('([0-9]{6} [0-9]{5})|([0-9]{11})',fodselsnummer) = true : 'Fødselsnummer må være 11 siffer, slik xxxxxxxxxxx eller slik xxxxxx xxxxxx' : 'validation.fodselsnummer' : fodselsnummer }
	    ]]>
	    </value>
	  </property>
	</bean>

		
</beans>