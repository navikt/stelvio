	</bean>
	<bean id="srv.starexample.errorDefinitionRetriever" class="no.stelvio.service.error.RepositoryErrorDefinitionRetriever">
		<property name="errorDefinitionRepository" ref="rep.starexample.errorDefinitionRepository"/>
	</bean>

