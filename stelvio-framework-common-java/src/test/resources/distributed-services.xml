<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "spring-beans.dtd">
<!--
	Distributed Services Configuration is used by the RemoteServiceLocator
-->
<beans>

	<bean id="TestEJBHome" class="no.nav.common.framework.ejb.RemoteServiceDescription">
		<property name="jndiName">
			<value>ejb/TestEJBHome</value>
		</property>
		<property name="providerUrl">
			<value>iiop://localhost:2809/</value>
		</property>
		<property name="initialContextFactory">
			<value>com.ibm.websphere.naming.WsnInitialContextFactory</value>
		</property>
		<property name="urlPkgPrefixes">
			<value>com.ibm.runtime:com.ibm.ws.naming:com.ibm.websphere.naming</value>
		</property>
		<property name="securityPrincipal">
			<value>username</value>
		</property>
		<property name="securityCredentials">
			<value>password</value>
		</property>
		<property name="cacheable">
			<value>true</value>
		</property>
	</bean>

	<bean id="IncompleteConfiguredFacade" class="no.nav.common.framework.ejb.RemoteServiceDescription"></bean>

</beans>

