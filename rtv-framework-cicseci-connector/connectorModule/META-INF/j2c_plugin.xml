<j2c_plugin tns="http://schemas.xmlsoap.org/wsdl/cicseci/"  name="cicseci">
	<Description>CICS ECI Connector Binding.</Description>
	<version>5.0</version>
	<ConnectionSpec classname="com.ibm.connector2.cics.ECIConnectionSpec"/>
	<InteractionSpec classname="com.ibm.connector2.cics.ECIInteractionSpec"/>
	<wsdl_extensions>  
		<address classname="com.ibm.connector2.cics.tools.ECIAddress" /> 
		<binding classname="com.ibm.connector2.cics.tools.ECIBinding" />  
		<operation classname="com.ibm.connector2.cics.tools.ECIOperation" />
		<input classname="com.ibm.connector2.cics.tools.ECIConnectionSpecProperty" />
		<input classname="com.ibm.connector2.cics.tools.ECIInteractionSpecProperty" />
		<output classname="com.ibm.connector2.cics.tools.ECIInteractionSpecProperty" />
		<extension_registry classname="com.ibm.connector2.cics.tools.ECIExtensionRegistry" />
	</wsdl_extensions>

	<wsif_extensions classname="com.ibm.connector2.cics.tools.WSIFProvider_ECI" />
	<formathandler>
		<generator encoding="ibmcobol"/>
		<generator encoding="ibmc"/>
		<generator encoding="ibmpli"/>
		<generator encoding="ibmhlasm"/>
	</formathandler>
</j2c_plugin>
