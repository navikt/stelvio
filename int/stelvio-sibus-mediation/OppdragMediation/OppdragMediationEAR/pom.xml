<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>no.nav.esb.layers.sibus-mediations</groupId>
		<artifactId>oppdrag-mediation</artifactId>
		<version>1.2-SNAPSHOT</version>
	</parent>

	<artifactId>oppdrag-mediation-ear</artifactId>
	<name>OppdragMediation EAR</name>
	<packaging>ear</packaging>

	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<artifactId>maven-ear-plugin</artifactId>
					<version>2.4</version>
					<configuration>
						<version>1.4</version>
						<displayName>
							${project.build.finalName}
						</displayName>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>

	<dependencies>
		<dependency>
			<groupId>com.ibm.ws.sib.api.jmsra</groupId>
			<artifactId>
				websphere_default_messaging_provider
			</artifactId>
			<type>rar</type>
			<version>6.1</version>
		</dependency>
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>oppdrag-mediation-ejb</artifactId>
			<type>ejb</type>
			<version>${project.version}</version>
		</dependency>
	</dependencies>
</project>