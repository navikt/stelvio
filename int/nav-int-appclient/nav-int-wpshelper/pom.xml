<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>no.stelvio.maven.poms</groupId>
		<artifactId>maven-root-pom</artifactId>
		<version>1.2.5</version>
	</parent>

	<groupId>no.nav.int.appclient.wpshelper</groupId>
	<artifactId>wpshelper</artifactId>
	<packaging>pom</packaging>
	<version>3.0.4-SNAPSHOT</version>
	<name>WPS Helper POM Module</name>

	<modules>
		<module>wpshelper-common-java</module>
		<module>femhelper-java</module>
		<module>femhelper-ear</module>
		<module>bpchelper-java</module>
		<module>bpchelper-ear</module>
		<module>sibushelper-java</module>
		<module>sibushelper-ear</module>
	</modules>

	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<artifactId>maven-ear-plugin</artifactId>
					<version>2.4.1</version>
				</plugin>
				<plugin>
					<artifactId>maven-release-plugin</artifactId>
					<version>2.5</version>
					<configuration>
						<autoVersionSubmodules>
							true
						</autoVersionSubmodules>
						<tagBase>
							https://versjonskontroll.adeo.no/svn/stelvio/tags/int
						</tagBase>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
		<extensions>
            <extension>
                <groupId>org.apache.maven.wagon</groupId>
                <artifactId>wagon-webdav-jackrabbit</artifactId>
                <version>1.0</version>
            </extension>
        </extensions>
	</build>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>${project.groupId}</groupId>
				<artifactId>wpshelper-common-java</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>${project.groupId}</groupId>
				<artifactId>femhelper-client</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>${project.groupId}</groupId>
				<artifactId>femhelper-ear</artifactId>
				<version>${project.version}</version>
				<type>ear</type>
			</dependency>
			<dependency>
				<groupId>${project.groupId}</groupId>
				<artifactId>bpchelper-client</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>${project.groupId}</groupId>
				<artifactId>bpchelper-ear</artifactId>
				<version>${project.version}</version>
				<type>ear</type>
			</dependency>
			<dependency>
				<groupId>${project.groupId}</groupId>
				<artifactId>sibushelper-client</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>${project.groupId}</groupId>
				<artifactId>sibushelper-ear</artifactId>
				<version>${project.version}</version>
				<type>ear</type>
			</dependency>

			<dependency>
				<groupId>org.slf4j</groupId>
				<artifactId>slf4j-api</artifactId>
				<version>${slf4j.version}</version>
			</dependency>
			<dependency>
				<groupId>org.slf4j</groupId>
				<artifactId>slf4j-jdk14</artifactId>
				<version>${slf4j.version}</version>
				<scope>runtime</scope>
			</dependency>
			<dependency>
				<groupId>commons-cli</groupId>
				<artifactId>commons-cli</artifactId>
				<version>1.2</version>
			</dependency>
			<dependency>
				<groupId>commons-lang</groupId>
				<artifactId>commons-lang</artifactId>
				<version>2.5</version>
			</dependency>
			<dependency>
				<groupId>commons-collections</groupId>
				<artifactId>commons-collections</artifactId>
				<version>3.2.1</version>
			</dependency>
			<dependency>
				<groupId>org.ostermiller</groupId>
				<artifactId>utils</artifactId>
				<version>1.07.00</version>
			</dependency>


			<dependency>
				<groupId>websphere</groupId>
				<artifactId>com.ibm.ws.ejb.thinclient</artifactId>
				<version>8.5.0</version>
				<scope>provided</scope>
			</dependency>
			<dependency>
				<groupId>websphere</groupId>
				<artifactId>com.ibm.ws.sib.client.thin.jms</artifactId>
				<version>8.5.0</version>
				<scope>provided</scope>
			</dependency>
			
			<dependency>
				<groupId>websphere</groupId>
				<artifactId>com.ibm.ws.admin.client</artifactId>
				<version>8.5.0</version>
				<scope>provided</scope>
			</dependency>
			
			<dependency>
				<groupId>websphere</groupId>
				<artifactId>com.ibm.ws.sib.server</artifactId>
				<version>8.5.0</version>
				<scope>provided</scope>
			</dependency>
			
			<dependency>
				<groupId>websphere</groupId>
				<artifactId>com.ibm.ws.runtime</artifactId>
				<version>8.5.0</version>
				<scope>provided</scope>
			</dependency>
			
			<dependency>
				<groupId>websphere</groupId>
				<artifactId>com.ibm.wbicmn.runtime</artifactId>
				<version>8.5.0</version>
				<scope>provided</scope>
			</dependency>

			<dependency>
				<groupId>org.eclipse.emf.commonj</groupId>
				<artifactId>org.eclipse.emf.commonj.sdo</artifactId>
				<version>2.1.0.v200609210005</version>
				<scope>provided</scope>
			</dependency>
			
			<dependency>
				<groupId>websphere.bpm</groupId>
				<artifactId>bfmclientmodel</artifactId>
				<version>8.5.0</version>
			</dependency>
			
			<dependency>
				<groupId>websphere.bpm</groupId>
				<artifactId>bpcclientcore</artifactId>
				<version>8.5.0</version>
			</dependency>
			
			<dependency>
				<groupId>websphere.bpm</groupId>
				<artifactId>bpeapi</artifactId>
				<version>8.5.0</version>
			</dependency>
			
			<dependency>
				<groupId>websphere.bpm</groupId>
				<artifactId>taskapi</artifactId>
				<version>8.5.0</version>
			</dependency>
			
			<dependency>
				<groupId>websphere.bpm</groupId>
				<artifactId>htmclientmodel</artifactId>
				<version>8.5.0</version>
			</dependency>
			
		</dependencies>
	</dependencyManagement>

	<scm>
		<connection>${scm.root}</connection>
		<developerConnection>${scm.root}</developerConnection>
	</scm>
	<properties>
		<slf4j.version>1.6.0</slf4j.version>
		<scm.root>
			scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/nav-int-appclient/nav-int-wpshelper
		</scm.root>
	</properties>

</project>