				<version>${project.version}</version>
				<type>ear</type>
			</dependency>
			<dependency>
				<groupId>${project.groupId}</groupId>
				<artifactId>sibushelper-client</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>${project.groupId}</groupId>
				<artifactId>sibushelper-ear</artifactId>
				<version>${project.version}</version>
				<type>ear</type>
			</dependency>


			<dependency>
