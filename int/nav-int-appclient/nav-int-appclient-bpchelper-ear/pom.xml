<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>nav.int.appclient.bpchelper</groupId>
		<artifactId>nav-int-appclient-bpc-pom</artifactId>
		<version>1.0-SNAPSHOT</version>
	</parent>
	
	<groupId>nav.int.appclient.bpchelper</groupId>
	<artifactId>bpchelper-ear</artifactId>
	<packaging>ear</packaging>
	<name>BPC-Helper EAR</name>

	<scm>
		<connection>
			https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/BPCCommandLine/BPCHelperAppClientEAR
		</connection>
		<developerConnection>
			https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/BPCCommandLine/BPCHelperAppClientEAR
		</developerConnection>
	</scm>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-ear-plugin</artifactId>
				<configuration>
					<displayName>nav-int-appclient-bpchelper-ear</displayName>
					<version>1.4</version>
					<modules>
						<jarModule>
							<groupId>
								nav.int.appclient.bpchelper
							</groupId>
							<artifactId>bpchelper-client</artifactId>
							<includeInApplicationXml>
								true
							</includeInApplicationXml>
						</jarModule>
					</modules>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<dependencies>
		<dependency>
			<groupId>nav.int.appclient.bpchelper</groupId>
			<artifactId>bpchelper-client</artifactId>
			<version>${project.version}</version>
		</dependency>
	</dependencies>
</project>
