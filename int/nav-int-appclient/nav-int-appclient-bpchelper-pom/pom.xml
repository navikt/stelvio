<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>nav.int.appclient.bpchelper</groupId>
	<artifactId>nav-int-appclient-bpc-pom</artifactId>
	<packaging>pom</packaging>
	<version>1.0-SNAPSHOT</version>
	<name>BPC-Helper Packaging</name>

	<scm>
		<connection>
			https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/BPCCommandLine/nav-int-appclient-bpc-pom
		</connection>
		<developerConnection>
			https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/BPCCommandLine/nav-int-appclient-bpc-pom
		</developerConnection>
	</scm>

	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-compiler-plugin</artifactId>
					<configuration>
						<source>1.5</source>
						<target>1.5</target>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>

	<modules>
		<module>../nav-int-appclient-bpchelper-java</module>
		<module>../nav-int-appclient-bpchelper-ear</module>
	</modules>
</project>
