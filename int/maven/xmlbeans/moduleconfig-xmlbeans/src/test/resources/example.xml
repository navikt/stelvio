<wps:configuration xmlns:wps="http://www.nav.no/pensjonsprogrammet/wpsconfiguration">
  <wps:security>
    <wps:authentication>
      <wps:inbound>
        <wps:tokens>
          <!--Zero or more repetitions:-->
          <wps:token>
            <wps:name>string</wps:name>
            <!--Optional:-->
            <wps:username>string</wps:username>
            <!--Optional:-->
            <wps:password>string</wps:password>
          </wps:token>
        </wps:tokens>
      </wps:inbound>
      <wps:outbound>
        <wps:tokens>
          <!--Zero or more repetitions:-->
          <wps:token>
            <wps:name>string</wps:name>
            <!--Optional:-->
            <wps:username>string</wps:username>
            <!--Optional:-->
            <wps:password>string</wps:password>
          </wps:token>
        </wps:tokens>
      </wps:outbound>
    </wps:authentication>
    <wps:authorization>
      <wps:roles>
        <!--Zero or more repetitions:-->
        <wps:role>
          <wps:name>string</wps:name>
          <!--Optional:-->
          <wps:everyone>string</wps:everyone>
          <!--Optional:-->
          <wps:allauthenticated>string</wps:allauthenticated>
          <wps:users>
            <!--Zero or more repetitions:-->
            <wps:user>
              <wps:name>string</wps:name>
            </wps:user>
          </wps:users>
          <wps:groups>
            <!--Zero or more repetitions:-->
            <wps:group>
              <wps:name>string</wps:name>
            </wps:group>
          </wps:groups>
          <!--Optional:-->
          <wps:runas>
            <wps:username>string</wps:username>
            <wps:password>string</wps:password>
          </wps:runas>
        </wps:role>
      </wps:roles>
    </wps:authorization>
  </wps:security>
  <wps:webservices>
    <wps:endpoints>
      <!--Zero or more repetitions:-->
      <wps:endpoint>
        <wps:name>string</wps:name>
        <wps:value>http://www.company.org/cum/sonoras</wps:value>
      </wps:endpoint>
    </wps:endpoints>
  </wps:webservices>
  <wps:runtime>
    <wps:autostart>string</wps:autostart>
    <wps:activationspecifications>
      <!--Zero or more repetitions:-->
      <wps:activationspecification>
        <wps:name>string</wps:name>
        <wps:maxconcurrency>2</wps:maxconcurrency>
      </wps:activationspecification>
    </wps:activationspecifications>
  </wps:runtime>
</wps:configuration>
