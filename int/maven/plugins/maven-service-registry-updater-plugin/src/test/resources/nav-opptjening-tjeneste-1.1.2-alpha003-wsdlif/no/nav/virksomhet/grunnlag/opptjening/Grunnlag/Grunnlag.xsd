  <xsd:complexType name="Omsorg">
    <xsd:annotation>
      <xsd:documentation>Omsorg vil etter Folketrygdloven kapittel 19 gi pensjonspoeng, og etter kapittel 20 (Alderspensjon2016 og Alderspensjon2025) gi pensjonsopptjening til pensjonsbeholdning. Omsorgsobjektet er grunnlagsobjekt på lik linje med inntekt, dagpenger og forstegangstjeneste, og det kan beregnes pensjonspoeng og pensjonsopptjening basert på den.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="omsorgId" nillable="true" type="xsd:long">
        <xsd:annotation>
          <xsd:documentation>Omsorg sin primærnøkkel</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="fnr" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Fødselsnummer til personen som har utført omsorgsarbeidet.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="fnrOmsorgFor" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Fødselsnummer til personen som har blitt forsørget.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="omsorgKode" type="grunnlag:OmsorgType"/>
      <xsd:element minOccurs="0" name="kilde" type="grunnlag:Kilde"/>
      <xsd:element name="ar" nillable="true" type="xsd:int">
        <xsd:annotation>
          <xsd:documentation>Året omsorgsarbeidet ble utført.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="endringsinfo" type="felles:Endringsinfo"/>
      <xsd:element minOccurs="0" name="utvidelse" type="grunnlag:OmsorgUtvidelse1"/>
    </xsd:sequence>
  </xsd:complexType>
