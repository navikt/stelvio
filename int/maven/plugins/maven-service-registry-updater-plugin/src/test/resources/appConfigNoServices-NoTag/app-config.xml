<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://appconfig.aura.nav.no" xmlns:service="http://appconfig.aura.nav.no/service"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://appconfig.aura.nav.no http://maven.adeo.no/xsd/appconfig.xsd ">
	<name>autodeploy-test</name>
	<artifacts>
		<ear groupId="no.nav.aura.test" artifactId="autodeploy-test-ear" />
	</artifacts>

	<!-- Dummyservice -->

	<resources>
		<datasource alias="autodeployTestAppDb" jndi="java:/jndi/jdbc/autodeployTestAppDs33">
			<pool minPoolSize="22" maxPoolSize="44" />
		</datasource>
		<datasource alias="autodeployTestAppDbDefaultPool" jndi="java:/jndi/jdbc/autodeployTestAppDsDefaultPool" />
		<baseUrl alias="autodeployTestAppBaseUrl" mapToProperty="autodeployTeset.baseUrl.esb" />
		<credential alias="autodeployTestAppWsUsername" mapToProperty="autodeployTest.esb.ws.credentials" />
		<unmanagedDatasource alias="autodeployTestAppUnmanagedDs" mapToProperty="autodeplyTest.unmanagedDb" />
	</resources>
</application>
