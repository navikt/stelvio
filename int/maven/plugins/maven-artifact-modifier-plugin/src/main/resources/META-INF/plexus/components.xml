<?xml version="1.0"?>
<component-set>
	<components>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>deployable-artifact</role-hint>
			<implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping</implementation>
			<configuration>
				<lifecycles>
					<lifecycle>
						<id>default</id>
						<phases>
							<initialize>
								no.nav.maven.plugins:maven-artifact-modifier-plugin:load-artifact-configuration,
								no.nav.maven.plugins:maven-websphere-plugin:load-runtime-configuration
							</initialize>
							<install>
								no.nav.maven.plugins:maven-artifact-modifier-plugin:modify-web-services-configuration,
								no.nav.maven.plugins:maven-artifact-modifier-plugin:modify-security-configuration
							</install>
							<deploy>
								no.nav.maven.plugins:maven-confluence-plugin:add-comment-down,
								no.nav.maven.plugins:maven-confluence-plugin:update-page,
								no.nav.maven.plugins:maven-websphere-plugin:backup-config,
								no.nav.maven.plugins:maven-websphere-plugin:remove-bp,
								no.nav.maven.plugins:maven-websphere-plugin:stop-clusters,
								no.nav.maven.plugins:maven-websphere-plugin:deploy-artifact,
								no.nav.maven.plugins:maven-websphere-plugin:deploy-resources,
								no.nav.maven.plugins:maven-websphere-plugin:role-mapping,
								no.nav.maven.plugins:maven-websphere-plugin:modify-imports,
								no.nav.maven.plugins:maven-websphere-plugin:apply-activationspecs,
								no.nav.maven.plugins:maven-websphere-plugin:apply-runtime-configuration,
								no.nav.maven.plugins:maven-websphere-plugin:synchronize-nodes,
								no.nav.maven.plugins:maven-websphere-plugin:start-clusters,
								no.nav.maven.plugins:maven-websphere-plugin:modify-max-failed-deliveries,
								no.nav.maven.plugins:maven-confluence-plugin:add-comment-up,
							</deploy>
						</phases>
					</lifecycle>
				</lifecycles>
			</configuration>
		</component>
		<component>
			<role>org.codehaus.plexus.components.interactivity.Prompter</role>
			<implementation>org.codehaus.plexus.components.interactivity.DefaultPrompter</implementation>
		</component>
	</components>
</component-set>
</xml>
