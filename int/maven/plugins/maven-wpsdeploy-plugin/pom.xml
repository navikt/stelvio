		<developerConnection>scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/tags/maven-wpsdeploy-plugin-3.0.2</developerConnection>
	</scm>
	<properties>
		<scm.root>
			scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/maven/plugins/maven-wpsdeploy-plugin
		</scm.root>
		<scm.connection>${scm.root}</scm.connection>
