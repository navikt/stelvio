<?xml version="1.0"?>
<component-set>
	<components>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>deployable-artifact</role-hint>
			<implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping</implementation>
			<configuration>
				<lifecycles>
					<lifecycle>
						<id>default</id>
						<phases>
						<validate>
							no.nav.maven.plugins:maven-wpsdeploy-plugin:extract-scripts,
							no.nav.maven.plugins:maven-wpsdeploy-plugin:extract-bus-configuration,
						</validate>
						<!-- <generate-sources> phase runs the "filter-resources" (interpolate EnvConfig) plugin in the deploy pom.xml -->
						<process-sources>
							no.nav.maven.plugins:maven-wpsdeploy-plugin:apply-fasit,
							no.nav.maven.plugins:maven-wpsdeploy-plugin:expose-properties,
						</process-sources>
						<!-- <process-resources> phase runs the "load-properties" (load passwords from vault) plugin in the deploy pom.xml -->
						<!-- <compile> phase runs the "copy-resources" (interpolate passwords) plugin in the deploy pom.xml -->
						<test>
							no.nav.maven.plugins:maven-wpsdeploy-plugin:apply-inheritance,	<!-- Aggregate the different properties files to one master file -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:interpolate-config,	<!-- Fill inn templates with values from properties -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:extract-module-poms,
							no.nav.maven.plugins:maven-wpsdeploy-plugin:generate-jython-path-script,	<!-- lib/configurationPaths.py
																										& lib/deployEnviromentUtil.py(auto generated) -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:dependency-identifier,	<!-- DependencyIdentifier.py -->
						</test>
						<deploy>
							no.nav.maven.plugins:maven-wpsdeploy-plugin:backup-config,		<!-- (SSH) -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:remove-bp,			<!-- RemoveOldBPModule.py -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:stop-clusters,		<!-- ClusterStop.py -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:delete-BLA-groups,	<!-- DeleteBLAGroups.py -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:deploy-artifacts,	<!-- ApplicationManagement.py -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:create-BLA-groups,	<!-- CreateBLAGroups.py -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:deploy-resources,	<!-- CreateApplicationArtifacts.py -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:set-environment-variables,	<!-- SetEnvironmentVariableDmgr.py -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:apply-rolemapping,	<!-- RoleMapping.py -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:apply-wsendpoints,	<!-- ModifySCAImportsBinding.py -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:apply-usernametokendetails,	<!-- ModifyUserNameTokenGenerator.py -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:apply-activationspecs,	<!-- ModifyMaxConcurrencyAS.py -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:synchronize-nodes,	<!-- ClusterSync.py -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:bounce-dmgr,		<!-- (SSH) Denne har vi pga VERA som er en versjonsoversikts side. Snakk med Roger bjÃ¸rnstad for mer info. -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:start-clusters,		<!-- ClusterStart.py -->
							no.nav.maven.plugins:maven-wpsdeploy-plugin:modify-max-failed-deliveries,	<!-- ModifyMaxFailedDeliveries.py -->
						</deploy>
						</phases>
					</lifecycle>
				</lifecycles>
			</configuration>
		</component>
		<component>
			<role>org.codehaus.plexus.components.interactivity.Prompter</role>
			<implementation>org.codehaus.plexus.components.interactivity.DefaultPrompter</implementation>
		</component>
	</components>
</component-set>
</xml>
