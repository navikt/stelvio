<?xml version="1.0"?>
<component-set>
	<components>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>deployable-artifact</role-hint>
			<implementation>org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping</implementation>
			<configuration>
				<lifecycles>
					<lifecycle>
						<id>default</id>
						<phases>
							<validate>
								no.nav.maven.plugins:maven-wpsdeploy-plugin:extract-scripts,
								no.nav.maven.plugins:maven-wpsdeploy-plugin:extract-bus-configuration,
								no.nav.maven.plugins:maven-wpsdeploy-plugin:interpolate-config,
							</validate>
							<!-- <process-sources> phase runs the "copy-resources" plugin in the deploy pom.xml -->
							<!-- <generate-resources> phase runs the "load-properties" plugin in the deploy pom.xml -->
							<compile>
								no.nav.maven.plugins:maven-wpsdeploy-plugin:load-configuration,
								no.nav.maven.plugins:maven-wpsdeploy-plugin:extract-module-poms,
							</compile>
							<deploy>
	 							no.nav.maven.plugins:maven-wpsdeploy-plugin:backup-config,
	 							no.nav.maven.plugins:maven-wpsdeploy-plugin:remove-bp,
	 							no.nav.maven.plugins:maven-wpsdeploy-plugin:stop-clusters,
								no.nav.maven.plugins:maven-wpsdeploy-plugin:deploy-artifact,
								no.nav.maven.plugins:maven-wpsdeploy-plugin:deploy-resources,
								no.nav.maven.plugins:maven-wpsdeploy-plugin:set-environment-variables,
								no.nav.maven.plugins:maven-wpsdeploy-plugin:apply-rolemapping,
								no.nav.maven.plugins:maven-wpsdeploy-plugin:apply-wsendpoints,
								no.nav.maven.plugins:maven-wpsdeploy-plugin:apply-usernametokendetails,
								no.nav.maven.plugins:maven-wpsdeploy-plugin:apply-activationspecs,
								no.nav.maven.plugins:maven-wpsdeploy-plugin:synchronize-nodes,
								no.nav.maven.plugins:maven-wpsdeploy-plugin:bounce-dmgr,
								no.nav.maven.plugins:maven-wpsdeploy-plugin:start-clusters,
								no.nav.maven.plugins:maven-wpsdeploy-plugin:modify-max-failed-deliveries,
							</deploy>
						</phases>
					</lifecycle>
				</lifecycles>
			</configuration>
		</component>
		<component>
			<role>org.codehaus.plexus.components.interactivity.Prompter</role>
			<implementation>org.codehaus.plexus.components.interactivity.DefaultPrompter</implementation>
		</component>
	</components>
</component-set>
</xml>