<?xml version="1.0"?>
<project>
	
	<target name="generatejava">		
		<property name="wsdlGenerateJava.dir" value="${project.build.directory}" />
		<property name="wsdlGenerateJava.input" value="${project.build.finalName}-sd.zip" />
		<condition property="wsdlGenerateJava.executable" value="${wid.runtime}/bin/WSDL2Java.bat" else="${wid.runtime}/bin/WSDL2Java.sh">
			<os family="windows" />
		</condition>
		<property name="wsdlGenerateJava.outputApplication" value="${project.build.finalName}.ear" />
		<property name="wsdlGenerateJava.workingDirectory" value="${project.build.directory}/service-deploy" />
	
		<echo message="Initiating wsdlGenerateJava with the following arguments: ${wsdlGenerateJava.arguments}" level="info" />
		<!-- exec dir="${wsdlGenerateJava.dir}" executable="${wsdlGenerateJava.executable}" failonerror="true">
			<arg line="${wsdlGenerateJava.arguments}" />
		</exec-->
		<exec dir="${wsdlGenerateJava.dir}" executable="${wsdlGenerateJava.executable}" failonerror="true">
			<arg line="-r deploy-client -c web -o e:\tmp -j Overwrite -D -f namespacemapping.properties *.wsdl" />
		</exec>
	</target>
</project>