<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd"
	xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="Stelvio Maven WPS Module Archetype">
	<requiredProperties>
		<requiredProperty key="serviceName" />
		<requiredProperty key="serviceScope" />
		<requiredProperty key="serviceScopeDashFormat" />
		<requiredProperty key="artifactId" >
			<defaultValue>${serviceName}</defaultValue>
		</requiredProperty>
		<requiredProperty key="groupId" >
			<defaultValue>no.nav.esb.${serviceScope}</defaultValue>
		</requiredProperty>
		<requiredProperty key="package">
			<defaultValue>no.nav.esb.${serviceScope}</defaultValue>
		</requiredProperty>
		<requiredProperty key="version">
			<defaultValue>1.0.0-alpha001-SNAPSHOT</defaultValue>
		</requiredProperty>
		<requiredProperty key="readableServiceName" />
		<requiredProperty key="serviceSpecArtifactId">
			<defaultValue>
				${serviceScope}-${serviceName}-tjenestespesifikasjon
			</defaultValue>
		</requiredProperty>
		<requiredProperty key="serviceSpecGroupId">
			<defaultValue>
				no.nav.tjenester
			</defaultValue>
		</requiredProperty>
		<requiredProperty key="serviceSpecVersion" />
		<requiredProperty key="wsdlNamespace" />
		<requiredProperty key="scmAddress">
			<defaultValue>
				https://versjonskontroll.adeo.no/svn/nav/trunk/int/${serviceScope}/${serviceName}
			</defaultValue>
		</requiredProperty>
	</requiredProperties>

	<fileSets>
		<fileSet filtered="true">
			<directory></directory>
			<includes>
				<include>**/*</include>
			</includes>
		</fileSet>
	</fileSets>
</archetype-descriptor>
