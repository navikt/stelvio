<?xml version="1.0" encoding="ISO-8859-1"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>no.stelvio.maven.poms</groupId>
		<artifactId>maven-plugin-pom</artifactId>
		<version>1.4</version>
		<relativePath>../../poms/maven-plugin-pom</relativePath>
	</parent>

	<groupId>no.nav.maven.plugins</groupId>
	<artifactId>maven-wid-plugin</artifactId>
	<packaging>maven-plugin</packaging>
	<version>3.5-SNAPSHOT</version>
	<name>Stelvio Integration Framework WID Plugin</name>

	<build>
		<plugins>
			<plugin>
				<groupId>org.ops4j</groupId>
				<artifactId>maven-inherit-plugin</artifactId>
				<version>1.1</version>
				<executions>
					<execution>
						<goals>
							<goal>inherit</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>org.codehaus.plexus</groupId>
			<artifactId>plexus-utils</artifactId>
		</dependency>
		
		<dependency>
			<groupId>org.apache.maven.shared</groupId>
			<artifactId>maven-invoker</artifactId>
		</dependency>

		<dependency>
			<groupId>org.apache.maven.plugins</groupId>
			<artifactId>maven-eclipse-plugin</artifactId>
			<type>maven-plugin</type>
			<version>${eclipse.plugin.version}</version>
		</dependency>
		
		<dependency>
			<groupId>no.nav.maven.support</groupId>
			<artifactId>sca-attributes-builder</artifactId>
			<version>2.3</version>
		</dependency>
	</dependencies>

	<scm>
		<connection>
			scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/maven/plugins/maven-wid-plugin
		</connection>
		<developerConnection>
			scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/maven/plugins/maven-wid-plugin
		</developerConnection>
	</scm>
	<properties>
		<scm.root>
			scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/maven/plugins/maven-wid-plugin
		</scm.root>
		<scm.connection>${scm.root}</scm.connection>
	</properties>
</project>
