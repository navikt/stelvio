        <xsd:simpleType>
          <xsd:restriction base="xsd:double">
            <xsd:minInclusive value="0"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="kravstillersReferanse">
