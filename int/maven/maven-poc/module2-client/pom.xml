<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>


	<artifactId>module2-client</artifactId>
	<groupId>no.nav.maven.poc</groupId>
	<version>1.0-SNAPSHOT</version>
	<packaging>jar</packaging>
	<name>Maven POC ${artifactId}</name>
	<build>
		<plugins>
			<plugin>
				<groupId>no.nav.maven.plugins</groupId>
				<artifactId>maven-wsdl-plugin</artifactId>
				<version>1.0-SNAPSHOT</version>
				<extensions>true</extensions>
				<executions>
					<execution>
						<goals>
							<goal>wsdl2java</goal>
						</goals>
						<configuration>
							<wsdlOptions>
								<wsdlOption>
									<wsdlArtifact>
										<groupId>no.nav.maven.poc</groupId>
										<artifactId>poc-module1</artifactId>
										<version>1.12-SNAPSHOT</version>
									</wsdlArtifact>
								</wsdlOption>
								<wsdlOption>
									<wsdlArtifact>
										<groupId>no.nav.maven.poc</groupId>
										<artifactId>poc-module2</artifactId>
										<version>1.10-SNAPSHOT</version>
									</wsdlArtifact>
								</wsdlOption>
							</wsdlOptions>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>ibm.rad70</groupId>
			<artifactId>thinclient</artifactId>
			<version>6.1.0</version>
			<scope>provided</scope>
		</dependency>
	</dependencies>
</project>
