<?xml version="1.0" encoding="ISO-8859-1"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>no.nav.maven.poc</groupId>
		<artifactId>modules</artifactId>
		<version>1.8-SNAPSHOT</version>
	</parent>

	<artifactId>cons</artifactId>
	<version>1.8-SNAPSHOT</version>
	<packaging>pom</packaging>
	<name>Maven POC ${artifactId}</name>

	<modules>
		<module>poc-module1</module>
		<module>poc-module2</module>
	</modules>

	<build>
		<plugins>
			<plugin>
				<artifactId>maven-wsdl-export-plugin</artifactId>
				<groupId>no.nav.maven.plugins</groupId>
				<version>1.1</version>
				<executions>
					<execution>
						<phase>package</phase>
						<goals>
							<goal>exportwsdl</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
</project>
