<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>test</groupId>
	<artifactId>test</artifactId>
	<version>1.0-SNAPSHOT</version>
	<packaging>pom</packaging>
	<name>test</name>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<wsadminLocation>/opt/IBM/WebSphere/ProcServer/profiles/Dmgr01/bin/</wsadminLocation>
	</properties>
	<build>
		<plugins>
			<plugin>
				<groupId>no.nav.testsenter-tools</groupId>
				<artifactId>maven-configure-wps-dev-plugin</artifactId>
				<version>1.0-SNAPSHOT</version>
				<executions>
					<execution>
						<id>configure-adapters</id>
						<phase>install</phase>
						<goals>
							<goal>configure-adapters</goal>
						</goals>
						<configuration>
							<adapters>
								<adapter>http://maven.adeo.no/nexus/content/groups/pp-releases/no/stelvio/int/jca-adapters/ADLDAPAdapter/7.0.2/ADLDAPAdapter-7.0.2.rar</adapter>
								<adapter>http://maven.adeo.no/nexus/content/groups/pp-releases/com/ibm/connector2/cics/cicseciXA/7.2.0.1/cicseciXA-7.2.0.1.rar</adapter>
								<adapter>http://maven.adeo.no/nexus/content/groups/pp-releases/com/ibm/connector2/cics/cicseciNoTrans/7.2.0.1/cicseciNoTrans-7.2.0.1.rar</adapter>
								<adapter>http://maven.adeo.no/nexus/content/groups/pp-releases/com/ibm/j2ca/email/CWYEM_EMail/7.0.0.3/CWYEM_EMail-7.0.0.3.rar</adapter>
							</adapters>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>


</project>
