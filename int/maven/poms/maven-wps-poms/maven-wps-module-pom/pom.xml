<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>no.stelvio.maven.poms</groupId>
		<artifactId>maven-wps-pom</artifactId>
		<version>2.1.14</version>
		<relativePath>../maven-wps-pom</relativePath>
	</parent>

	<artifactId>maven-wps-module-pom</artifactId>
	<version>2.1.15-SNAPSHOT</version>
	<packaging>pom</packaging>
	<name>Stelvio Integration Framework WPS Module Master POM</name>

	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<artifactId>maven-jar-plugin</artifactId>
					<configuration>
						<classifier>jar</classifier>
						<archive>
							<manifest>
								<addDefaultSpecificationEntries>true</addDefaultSpecificationEntries>
								<addDefaultImplementationEntries>true</addDefaultImplementationEntries>
							</manifest>
							<manifestEntries>
								<Class-Path>${manifest.classpath}</Class-Path>
							</manifestEntries>
						</archive>
					</configuration>
				</plugin>
				<plugin>
					<groupId>no.nav.maven.plugins</groupId>
					<artifactId>maven-service-deploy-plugin</artifactId>
					<version>2.4</version>
				</plugin>
				<plugin>
					<groupId>no.nav.maven.plugins</groupId>
					<artifactId>maven-wid-plugin</artifactId>
					<configuration>
						<workspaceCodeStylesURL>${eclipse.workspaceCodeStylesURL}</workspaceCodeStylesURL>
						<downloadJavadocs>true</downloadJavadocs>
						<downloadSources>true</downloadSources>
						<buildcommands>
							<buildCommand>
								<name>org.eclipse.wst.common.project.facet.core.builder</name>
							</buildCommand>
							<buildCommand>
								<name>com.ibm.wbit.project.wbimodulebuilder_prejdt</name>
							</buildCommand>
							<buildCommand>
								<name>org.eclipse.jdt.core.javabuilder</name>
							</buildCommand>
							<buildCommand>
								<name>com.ibm.wbit.project.wbimodulebuilder_postjdt</name>
							</buildCommand>
						</buildcommands>
						<projectnatures>
							<projectnature>org.eclipse.wst.common.project.facet.core.nature</projectnature>
							<projectnature>org.eclipse.jdt.core.javanature</projectnature>
							<projectnature>org.eclipse.wst.common.modulecore.ModuleCoreNature</projectnature>
							<projectnature>org.eclipse.jem.workbench.JavaEMFNature</projectnature>
							<projectnature>com.ibm.ws.sca.rapiddeploy.style.SCAProjectNature</projectnature>
							<projectnature>com.ibm.wbit.project.generalmodulenature</projectnature>
						</projectnatures>
						<runtimeName>IBM Process Server v8.5</runtimeName>
						<classpathContainers>
							<classpathContainer>
								<![CDATA[
								org.eclipse.jdt.launching.JRE_CONTAINER/org.eclipse.jdt.internal.debug.ui.launcher.StandardVMType/IBM Process Server v8.5 JRE
								]]>
							</classpathContainer>
							<classpathContainer>org.eclipse.jst.server.core.container/com.ibm.ws.ast.st.runtime.runtimeTarget.v85.bi/wps.v85</classpathContainer>
						</classpathContainers>
						<excludes>
							<exclude>com.ibm.maven.runtimes:wps-runtime</exclude>
							<exclude>com.ibm.ws.runtime:WS_Server</exclude>
							<exclude>websphere:com.ibm.jaxws.thinclient</exclude>
							<exclude>websphere:com.ibm.ws.admin.client</exclude>
						</excludes>
						<additionalConfig>
							<file>
								<name>.settings/com.ibm.ws.sca.rapiddeploy.style.prefs</name>
								<content>
									<![CDATA[
config.version=6.1
eclipse.preferences.version=1
									]]>
								</content>
							</file>
							<file>
								<name>
									.factorypath
								</name>
								<content>
									<![CDATA[
<factorypath>
    <factorypathentry kind="PLUGIN" id="com.ibm.ws.ast.wsfp.annotations.processor" enabled="true" runInBatchMode="false" />
    <factorypathentry kind="PLUGIN" id="com.ibm.adapter.j2c.codegen" enabled="true" runInBatchMode="false" />
    <factorypathentry kind="PLUGIN" id="com.ibm.jee.ejb.annotations.processor" enabled="false" runInBatchMode="false" />
</factorypath>
									]]>
								</content>
							</file>
						</additionalConfig>
					</configuration>
				</plugin>
				<plugin>
					<groupId>no.nav.maven.plugins</groupId>
					<artifactId>maven-update-ear-plugin</artifactId>
					<version>1.13</version>
					<executions>
						<execution>
							<goals>
								<goal>update-ear</goal>
							</goals>
							<phase>package</phase>
						</execution>
					</executions>
				</plugin>
				<plugin>
					<groupId>no.nav.maven.plugins</groupId>
					<artifactId>maven-wsdl-plugin</artifactId>
					<version>1.18</version>
					<executions>
						<execution>
							<phase>package</phase>
							<goals>
								<goal>exportwsdl</goal>
							</goals>
						</execution>
					</executions>
				</plugin>
			</plugins>
		</pluginManagement>
		<plugins>
			<plugin>
				<groupId>no.nav.maven.plugins</groupId>
				<artifactId>maven-update-ear-plugin</artifactId> 
			</plugin>
			<plugin>
				<groupId>no.nav.maven.plugins</groupId>
				<artifactId>maven-wsdl-plugin</artifactId>
			</plugin>
		</plugins>
	</build>

	<scm>
		<connection>scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/maven/poms/maven-wps-poms/maven-wps-module-pom</connection>
		<developerConnection>scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/maven/poms/maven-wps-poms/maven-wps-module-pom</developerConnection>
	</scm>
	<properties>
		<scm.root>
			scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/maven/poms/maven-wps-poms/maven-wps-module-pom
		</scm.root>
		<scm.connection>${scm.root}</scm.connection>
	</properties>
</project>
