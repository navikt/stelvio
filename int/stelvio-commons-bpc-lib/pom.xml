<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>no.stelvio.maven.poms</groupId>
		<artifactId>maven-wps-library-pom</artifactId>
		<version>1.0-SNAPSHOT</version>
	</parent>

	<groupId>no.stelvio.int</groupId>
	<artifactId>stelvio-commons-bpc-lib</artifactId>
	<packaging>wps-library-jar</packaging>
	<version>1.3-SNAPSHOT</version>
	<name>Stelvio Integration Framework : ${artifactId}</name>
	<description>
		This servicelibrary contains common java classes and business
		objects for use by WPS modules and libraries.
	</description>

	<dependencies>
		<dependency>
			<groupId>com.ibm.wps</groupId>
			<artifactId>bfmclientmodel</artifactId>
			<version>6.1.0</version>
		</dependency>
		<dependency>
			<groupId>com.ibm.wps</groupId>
			<artifactId>bpcclientcore</artifactId>
			<version>6.1.0</version>
		</dependency>
		<dependency>
			<groupId>com.ibm.wps</groupId>
			<artifactId>bpe137650</artifactId>
			<version>6.1.0</version>
		</dependency>
		<dependency>
			<groupId>com.ibm.wps</groupId>
			<artifactId>bpeapi</artifactId>
			<version>6.1.0</version>
		</dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-release-plugin</artifactId>
				<configuration>
					<tagBase>
						https://versjonskontroll.adeo.no/svn/stelvio/tags/int
					</tagBase>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<scm>
		<connection>
			scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/stelvio-commons-bpc-lib
		</connection>
		<developerConnection>
			scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/stelvio-commons-bpc-lib
		</developerConnection>
	</scm>
</project>
