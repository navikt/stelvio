<?xml version="1.0" encoding="UTF-8"?>
<emd:discoveryService xmlns:emd="commonj.connector"
	xmlns:j2ee="http://java.sun.com/xml/ns/j2ee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/j2ee_1_4.xsd">
	<j2ee:description>IBM WebSphere Adapter for Flat Files</j2ee:description>
	<j2ee:display-name>IBM WebSphere Adapter for Flat Files</j2ee:display-name>
	<emd:vendor-name xsi:type="j2ee:xsdStringType">IBM</emd:vendor-name>
	<emd:version xsi:type="j2ee:xsdStringType">6.0.2.1</emd:version>
	<emd:spec-version>1.0</emd:spec-version>
	<emd:discoveryService-class
		xsi:type="j2ee:fully-qualified-classType">
		com.ibm.j2ca.flatfile.emd.discovery.FlatFileMetadataDiscoveryImpl
	</emd:discoveryService-class>
	<emd:metadataEdit-class xsi:type="j2ee:fully-qualified-classType">
		com.ibm.j2ca.flatfile.emd.discovery.FlatFileMetadataEdit
	</emd:metadataEdit-class>
	<emd:application-specific-schema>
		<j2ee:description>Flat Files ASI schema</j2ee:description>
		<j2ee:display-name>Flat Files ASI schema</j2ee:display-name>
		<emd:asiNSURI>
			http://www.ibm.com/xmlns/prod/websphere/j2ca/datatransformation/databindingmapping
		</emd:asiNSURI>
		<emd:asiSchemaLocation>FlatFileASI.xsd</emd:asiSchemaLocation>
	</emd:application-specific-schema>
</emd:discoveryService>