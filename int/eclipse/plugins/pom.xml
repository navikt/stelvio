<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>no.stelvio.maven.poms</groupId>
		<artifactId>maven-root-pom</artifactId>
		<version>1.2.5</version>
	</parent>

	<groupId>no.stelvio.int.models</groupId>
	<artifactId>stelvio-models-poms</artifactId>
	<packaging>pom</packaging>
	<version>1.0-SNAPSHOT</version>
	<name>Stelvio Enterprise Platform Integration ${artifactId}</name>

	<modules>
		<module>stelvio-models-service-metamodel</module>
		<module>stelvio-models-service-uml-profile</module>
		<module>stelvio-models-transformation-diagram2img</module>
		<module>stelvio-models-transformation-uml2servicemodel</module>
		<module>stelvio-models-transformation-servicemodel2html</module>
		<module>stelvio-models-transformation-uml2html</module>
	</modules>
	
	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<artifactId>maven-release-plugin</artifactId>
					<configuration>
						<tagBase>
							https://versjonskontroll.adeo.no/svn/stelvio/tags/int/eclipse
						</tagBase>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>
	
	<scm>
		<connection>scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/eclipse/plugins</connection>
		<developerConnection>scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/eclipse/plugins</developerConnection>
	</scm>
	<properties>
		<scm.root>scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/int/eclipse/plugins</scm.root>
		<scm.connection>${scm.root}</scm.connection>
	</properties>
</project>