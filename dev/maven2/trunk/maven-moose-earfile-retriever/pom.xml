<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  	<modelVersion>4.0.0</modelVersion>
  	<groupId>no.nav.maven.plugins</groupId>
  	<artifactId>maven-moose-earfile-retriever</artifactId>
  	<packaging>jar</packaging>
  	<version>1.0.0-SNAPSHOT</version>
  	<name>Maven Moose Ear file retriever. Utility Jar to retrieve the ear file from a moose ID and a moose mapping file</name>
  	<description>
  	</description>>
  	<url>http://maven.apache.org</url>
  	<dependencies>
  	    <dependency>
			<artifactId>spring-core</artifactId>
			<groupId>org.springframework</groupId>
			<version>2.0.4</version>
		</dependency>
		<dependency>
			<!-- spring-beans is a Spring core library used for manipulating Java beans. This is the most basic Spring library. -->
			<artifactId>spring-beans</artifactId>
			<groupId>org.springframework</groupId>
			<version>2.0.4</version>
		</dependency>
		<dependency>
			<!-- spring-context is a Spring core library used for configuration and dependency injection. Builds on the spring-beans library. -->
			<artifactId>spring-context</artifactId>
			<groupId>org.springframework</groupId>
			<version>2.0.4</version>
		</dependency>
    	<dependency>
			<artifactId>commons-logging</artifactId>
			<groupId>commons-logging</groupId>
			<version>1.1</version>
		</dependency>
  	</dependencies>
  	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	
	<distributionManagement>
       	<repository>
            <id>nav-maven2-internal-repo</id>
            <name>NAV Maven2 Internal Repository</name>
            <url>scp://maven.adeo.no/var/maven2/internal</url>
        </repository>
        <snapshotRepository>
            <id>nav-pensjon-maven2-snapshot-repo</id>
            <name>NAV Maven2 Internal Snapshot Repository</name>
            <url>scp://10.33.1.53/var/maven2/snapshot</url>
        </snapshotRepository>
    </distributionManagement>
</project>
