<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  	<modelVersion>4.0.0</modelVersion>
  	<groupId>no.nav.maven.plugins</groupId>
  	<artifactId>maven-datapower-deploy-plugin-test</artifactId>
  	<packaging>jar</packaging>
  	<version>1.0-SNAPSHOT</version>
  	<name>Test Maven DataPower Plugin</name>

  	<dependencies>
    	<dependency>
      		<groupId>junit</groupId>
      		<artifactId>junit</artifactId>
      		<version>3.8.1</version>
      		<scope>test</scope>
    	</dependency>
  	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>no.nav.maven.plugins</groupId>
				<artifactId>maven-datapower-deploymentpolicy-plugin</artifactId>
				<version>1.0-SNAPSHOT</version>
				<configuration>
					<domain>es-gw-deployment</domain>
<!-- 				<deviceUrl>https://signature-gw-02.test.local:5550/service/mgmt/current</deviceUrl>
	-->
					<deviceUrl>https://dp-tst-11.adeo.no:5550</deviceUrl>		
					<xmlMgmtEndpoint>/service/mgmt/current</xmlMgmtEndpoint>		
					<username>t115270</username>
					<password></password>
					<trustStorePath>${basedir}/src/test/resources/trustStore.jks</trustStorePath>
					<trustStorePwd>Passord1234</trustStorePwd>
					<httpClientClass>no.nav.datapower.xmlmgmt.net.DPHttpClientPrintOut</httpClientClass>
				</configuration>
				<executions>
					<execution>
						<id>createDomain</id>
						<phase>compile</phase>
						<goals><goal>createDomain</goal></goals>	
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
</project>
