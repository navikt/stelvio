<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>no.nav.maven.plugins</groupId>
	<artifactId>maven-deployment-descriptor-plugin-test</artifactId>
	<packaging>jar</packaging>
	<version>1.0-SNAPSHOT</version>
	<name>Test Maven WebSphere Deployment Descriptor Plugin</name>

	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>3.8.1</version>
			<scope>test</scope>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>no.nav.maven.plugins</groupId>
				<artifactId>
					maven-deployment-descriptor-plugin
				</artifactId>
				<configuration>
					<ear>
						${basedir}/target/test-classes/ear/nav-cons-pen-pselv-person-NOTOKENS-test.ear
					</ear>
					<authMechanism>WSSBoth</authMechanism>

				</configuration>
				<executions>
					<id>addWsSecurityInbound</id>
					<execution>
						<configuration></configuration>
						<goal>addWsSecurityInbound</goal>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
</project>
