<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  	<modelVersion>4.0.0</modelVersion>
  	<groupId>no.nav.maven.plugins</groupId>
  	<artifactId>maven-deployment-descriptor-plugin</artifactId>
  	<packaging>maven-plugin</packaging>
  	<version>1.0.1-SNAPSHOT</version>
  	<name>Maven WebSphere Deployment Descriptor Plugin</name>
  	<description>
  	</description>>
  	<url>http://maven.apache.org</url>
  	<dependencies>
    	<dependency>
      		<groupId>org.apache.maven</groupId>
      		<artifactId>maven-plugin-api</artifactId>
      		<version>2.0</version>
    	</dependency>
    	<dependency>
    		<groupId>no.nav.maven.plugins</groupId>
      		<artifactId>maven-deployment-descriptor-common</artifactId>
      		<version>1.0.1-SNAPSHOT</version>
    	</dependency>
    	
    	<!-- Test scoped dependencies -->
    	<dependency>
      		<groupId>junit</groupId>
      		<artifactId>junit</artifactId>
      		<version>3.8.1</version>
      		<scope>test</scope>
    	</dependency>
    	<dependency>
      		<groupId>org.apache.maven.plugin-testing</groupId>
      		<artifactId>maven-plugin-testing-harness</artifactId>
      		<version>1.2</version>
      		<scope>test</scope>
    	</dependency>
  	</dependencies>
  	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	
	<distributionManagement>
       	<repository>
            <id>nav-maven2-internal-repo</id>
            <name>NAV Maven2 Internal Repository</name>
            <url>scp://maven.adeo.no/var/maven2/internal</url>
        </repository>
        <snapshotRepository>
            <id>nav-pensjon-maven2-snapshot-repo</id>
            <name>NAV Maven2 Internal Snapshot Repository</name>
            <url>scp://10.33.1.53/var/maven2/snapshot</url>
        </snapshotRepository>
    </distributionManagement>
</project>
