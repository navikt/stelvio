<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  	<modelVersion>4.0.0</modelVersion>
  	<groupId>no.nav.maven.plugins</groupId>
  	<artifactId>maven-datapower-plugin-test</artifactId>
  	<packaging>jar</packaging>
  	<version>1.0.1-SNAPSHOT</version>
  	<name>Test Maven DataPower Plugin</name>

	
  	<dependencies>
    	<dependency>
      		<groupId>junit</groupId>
      		<artifactId>junit</artifactId>
      		<version>3.8.1</version>
      		<scope>test</scope>
    	</dependency>
  	</dependencies>
	<properties>
		<bustag.basedir>E:\data\builds\BUSTAG_R_3_8_0_FULL</bustag.basedir>
	</properties>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-clean-plugin</artifactId>
				<configuration>
					<filesets>
						<fileset>
							<directory>E:/data/builds/BUSTAG_R_3_8_0_FULL/target/datapower-config/secgw-u1</directory>
						</fileset>
					</filesets>					
				</configuration>
				<executions>
					<execution>
						<id>clean</id>
						<phase>validate</phase>
						<goals><goal>clean</goal></goals>
					</execution>
				</executions>
			</plugin>						
			<plugin>
				<groupId>no.nav.maven.plugins</groupId>
				<artifactId>maven-datapower-plugin</artifactId>
				<version>LATEST</version>
				<configuration>
					<domain>secgw-u1</domain>
					<outputDirectory>E:/data/builds/BUSTAG_R_3_8_0_FULL/target/datapower-config/secgw-u1</outputDirectory>
					<moduleDirectory>E:/data/builds/BUSTAG_R_3_8_0_FULL/target/classes/builds/ear</moduleDirectory>
					<propertiesDirectory>E:/data/builds/BUSTAG_R_3_8_0_FULL/src/main/resources/scripts/environments</propertiesDirectory>
					<overridesDirectory>E:/data/builds/datapower-overrides/secgw-u1</overridesDirectory>
				</configuration>
				<executions>
					<execution>
						<id>generateConfig</id>
						<phase>compile</phase>
						<goals><goal>generateConfig</goal></goals>
					</execution>
				</executions>
				<dependencies>
					<dependency>
						<groupId>no.nav.datapower</groupId>
						<artifactId>datapower-cfg-secgw</artifactId>
						<version>LATEST</version>
					</dependency>
				</dependencies>
			</plugin>
		</plugins>
	</build>
</project>
