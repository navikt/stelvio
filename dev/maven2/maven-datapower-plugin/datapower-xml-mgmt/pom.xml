<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>no.nav.maven.plugins</groupId>
		<artifactId>maven-datapower-plugin-project</artifactId>
		<version>1.1.11</version>
	</parent>

	<scm>
		<connection>
			scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/tags/maven-datapower-plugin-project-1.1.11/datapower-xml-mgmt
		</connection>
		<developerConnection>
			scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/tags/maven-datapower-plugin-project-1.1.11/datapower-xml-mgmt
		</developerConnection>
	</scm>

	<artifactId>datapower-xml-mgmt</artifactId>
	<packaging>jar</packaging>

	<name>DataPower XML Management Interface Library</name>
	<dependencies>
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>datapower-utils</artifactId>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<distributionManagement>
		<repository>
			<id>nav-maven2-internal-repo</id>
			<name>NAV Maven2 Internal Repository</name>
			<url>scp://maven.adeo.no/var/maven2/internal</url>
		</repository>
		<snapshotRepository>
			<id>nav-pensjon-maven2-snapshot-repo</id>
			<name>NAV Maven2 Internal Snapshot Repository</name>
			<url>scp://maven.adeo.no/var/maven2/snapshot</url>
		</snapshotRepository>
	</distributionManagement>
</project>
