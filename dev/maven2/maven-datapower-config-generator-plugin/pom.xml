<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>no.nav.maven.plugins</groupId>
	<artifactId>maven-datapower-config-generator-plugin</artifactId>
	<packaging>maven-plugin</packaging>
	<version>1.2</version>

	<name>Maven Datapower Config Generator Plugin</name>
	<description>This plugin generates DataPower configuration based on predefined templates and specified WSDL files.</description>

	<dependencies>
		<dependency>
			<groupId>org.apache.maven</groupId>
			<artifactId>maven-plugin-api</artifactId>
			<version>2.0</version>
		</dependency>
		<dependency>
			<groupId>org.apache.maven</groupId>
			<artifactId>maven-project</artifactId>
			<version>2.0</version>
		</dependency>
		<dependency>
			<groupId>org.freemarker</groupId>
			<artifactId>freemarker</artifactId>
			<version>2.3.15</version>
		</dependency>
		<dependency>
			<groupId>no.nav.maven.plugins</groupId>
			<artifactId>datapower-utils</artifactId>
			<version>1.1.15</version>
		</dependency>
	</dependencies>

	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.0.2</version>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<scm>
		<connection>scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/tags/maven-datapower-config-generator-plugin-1.2</connection>
		<developerConnection>scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/tags/maven-datapower-config-generator-plugin-1.2</developerConnection>
	</scm>
</project>
