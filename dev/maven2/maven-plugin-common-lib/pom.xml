<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>no.nav.maven.plugins</groupId>
	<artifactId>maven-plugin-common-lib</artifactId>
	<packaging>jar</packaging>
	<version>1.2</version>
	<name>maven-plugin-common-lib</name>
	<url>http://maven.apache.org</url>
	<dependencies>
		<dependency>
	        <groupId>de.schlichtherle.io</groupId>
	        <artifactId>truezip</artifactId>
	        <version>6.6</version>
	    </dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>3.8.1</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.apache.maven</groupId>
			<artifactId>maven-plugin-api</artifactId>
			<version>2.0</version>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>3.8.1</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>dom4j</groupId>
			<artifactId>dom4j</artifactId>
			<version>1.6.1</version>
		</dependency>
		<dependency>
			<groupId>jaxen</groupId>
			<artifactId>jaxen</artifactId>
			<version>1.1</version>
		</dependency>
		<dependency>
			<groupId>sax</groupId>
			<artifactId>sax</artifactId>
			<version>2.0.1</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<distributionManagement>
		<repository>
			<id>nav-maven2-internal-repo</id>
			<name>NAV Maven2 Internal Repository</name>
			<url>scp://maven.adeo.no/var/maven2/internal</url>
		</repository>
		<snapshotRepository>
			<id>nav-pensjon-maven2-snapshot-repo</id>
			<name>NAV Maven2 Internal Snapshot Repository</name>
			<url>scp://maven.adeo.no/var/maven2/snapshot</url>
		</snapshotRepository>
		<site>
			<id>website</id>
			<url>scp://10.33.1.53/var/www/html/project/maven/plugins/configfixer/</url>
		</site>
	</distributionManagement>	
</project>
