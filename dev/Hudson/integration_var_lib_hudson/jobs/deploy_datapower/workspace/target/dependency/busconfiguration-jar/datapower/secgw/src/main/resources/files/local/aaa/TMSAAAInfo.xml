<aaa:AAAInfo xmlns:dpfunc="http://www.datapower.com/extensions/functions" xmlns:aaa="http://www.datapower.com/AAAInfo">
	<aaa:FormatVersion>1</aaa:FormatVersion>
	<aaa:Filename>TMSAAAInfo.xml</aaa:Filename>
	<aaa:Summary>Access control file for traffic from SBL to TMS</aaa:Summary>
	<!-- Determine credential from output of the extract-identity phase. -->
	<!-- Specify credential (if any) to use when there is no authenticated identity. -->
	<aaa:Authenticate/>	
	<!-- Map credentials to different credentials. -->
	<aaa:MapCredentials>
        <aaa:InputCredential>${TMSfrontsideUserLC}</aaa:InputCredential>
        <aaa:OutputCredential>
			<entry type="wssec-username">
				<username>${TMSbacksideUsername}</username> 
				<password type="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-username-token-profile-1.0#PasswordText" sanitize="false">${TMSbacksidePassword}</password> 
			</entry>		
		</aaa:OutputCredential>
    </aaa:MapCredentials>
	<aaa:MapCredentials>
        <aaa:InputCredential>${TMSfrontsideUserUC}</aaa:InputCredential>
        <aaa:OutputCredential>
			<entry type="wssec-username">
				<username>${TMSbacksideUsername}</username> 
				<password type="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-username-token-profile-1.0#PasswordText" sanitize="false">${TMSbacksidePassword}</password> 
			</entry>		
		</aaa:OutputCredential>
    </aaa:MapCredentials>
	<!-- Determine resource from output of the extract-resource phase. -->
	<!-- Authorize access to resource for credentials. -->
	<aaa:Authorize/>
</aaa:AAAInfo>