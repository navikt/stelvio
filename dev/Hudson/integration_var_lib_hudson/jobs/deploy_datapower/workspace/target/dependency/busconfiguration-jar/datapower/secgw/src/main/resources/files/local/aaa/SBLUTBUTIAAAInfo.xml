<aaa:AAAInfo xmlns:dpfunc="http://www.datapower.com/extensions/functions" xmlns:aaa="http://www.datapower.com/AAAInfo">
	<aaa:FormatVersion>1</aaa:FormatVersion>
	<aaa:Filename>SBLUTBUTIAAAInfo.xml</aaa:Filename>
	<aaa:Summary>Access control file for traffic from SBL to UTI</aaa:Summary>
	<!-- Determine credential from output of the extract-identity phase. -->
	<!-- Specify credential (if any) to use when there is no authenticated identity. -->
	<aaa:Authenticate>
		<!-- DineUtbetalinger user, lower case variant -->
		<aaa:Username>${SBLUTBUTIfrontsideUserLC}</aaa:Username> 
		<aaa:OutputCredential>SBLUTBUTIUser</aaa:OutputCredential>
	</aaa:Authenticate>
	<aaa:Authenticate>
		<!-- DineUtbetalinger, upper case variant -->
		<aaa:Username>${SBLUTBUTIfrontsideUserUC}</aaa:Username> 
		<aaa:OutputCredential>SBLUTBUTIUser</aaa:OutputCredential>
	</aaa:Authenticate>	
	<!-- Map credentials to different credentials. -->
	<!-- Determine resource from output of the extract-resource phase. -->
	<!-- Authorize access to resource for credentials. -->
	<aaa:Authorize>
		<!-- allow acces to UTI service for DineUtbetalinger consumer, lower case variant-->
		<aaa:InputCredential>${SBLUTBUTIfrontsideUserLC}</aaa:InputCredential>
		<aaa:InputResource>${SBLUTBUTIURIDineutb}</aaa:InputResource>
		<aaa:Access>allow</aaa:Access>
	</aaa:Authorize>
	<aaa:Authorize>
		<!-- allow acces to UTI service for DineUtbetalinger consumer, upper case variant -->
		<aaa:InputCredential>${SBLUTBUTIfrontsideUserUC}</aaa:InputCredential>
		<aaa:InputResource>${SBLUTBUTIURIDineutb}</aaa:InputResource>
		<aaa:Access>allow</aaa:Access>
	</aaa:Authorize>
</aaa:AAAInfo>