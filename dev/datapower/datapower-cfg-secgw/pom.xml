<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>no.nav.datapower</groupId>
	<artifactId>datapower-cfg-secgw</artifactId>
	<packaging>jar</packaging>
	<version>1.0.0</version>
	<name>Configuration Generator for the 'secgw' DataPower module</name>
	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>3.8.1</version>
			<scope>test</scope>
		</dependency>
		<dependency>
	        <groupId>log4j</groupId>
	        <artifactId>log4j</artifactId>
	        <version>1.2.12</version>
	    </dependency>
		<dependency>
	        <groupId>no.nav.datapower</groupId>
	        <artifactId>datapower-utils</artifactId>
	        <version>1.0.0</version>
	    </dependency>
		<dependency>
	        <groupId>no.nav.datapower</groupId>
	        <artifactId>datapower-cfg-gen</artifactId>
	        <version>1.0.0</version>
	    </dependency>
    	<!--  
    	<dependency>
      		<groupId>no.nav.maven.plugins</groupId>
      		<artifactId>maven-websphere-plugin-deps</artifactId>
      		<version>1.0-SNAPSHOT</version>
    	</dependency>-->
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
		</plugins>
	</build>	
	<distributionManagement>
		<repository>
			<id>nav-maven2-internal-repo</id>
			<name>NAV Maven2 Internal Repository</name>
			<url>scp://maven.adeo.no/var/maven2/internal</url>
		</repository>
		<snapshotRepository>
			<id>nav-pensjon-maven2-snapshot-repo</id>
			<name>NAV Maven2 Internal Snapshot Repository</name>
			<url>scp://maven.adeo.no/var/maven2/snapshot</url>
		</snapshotRepository>
	</distributionManagement>
</project>
