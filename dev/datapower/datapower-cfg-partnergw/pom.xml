<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	
	<groupId>no.nav.datapower</groupId>
	<artifactId>datapower-cfg-partnergw</artifactId>
	<packaging>jar</packaging>
	<version>1.0.9</version>
	<name>Configuration Generator for the 'partner-gw' DataPower module</name>
	
	<scm>
		<connection>scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/tags/datapower-cfg-partnergw-1.0.9</connection>
		<developerConnection>scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/tags/datapower-cfg-partnergw-1.0.9</developerConnection>
	</scm>
	
	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-compiler-plugin</artifactId>
					<configuration>
						<source>1.5</source>
						<target>1.5</target>
					</configuration>
				</plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-release-plugin</artifactId>
					<configuration>
						<preparationGoals>
							clean install
						</preparationGoals>
						<autoVersionSubmodules>
							true
						</autoVersionSubmodules>
						<tagBase>
							https://versjonskontroll.adeo.no/svn/stelvio/tags
						</tagBase>
					</configuration>
				</plugin>
				<plugin>
					<artifactId>maven-eclipse-plugin</artifactId>
					<configuration>
						<workspaceCodeStylesURL>
							http://confluence.adeo.no/download/attachments/30647/Nav-Formatter-0.1.xml?version=1
						</workspaceCodeStylesURL>
						<buildOutputDirectory>bin</buildOutputDirectory>
						<downloadJavadocs>true</downloadJavadocs>
						<downloadSources>true</downloadSources>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
		
		<plugins>
			<plugin>
				<artifactId>maven-source-plugin</artifactId>
				<executions>
					<execution>
						<id>attach-sources</id>
						<phase>verify</phase>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
	
	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>3.8.1</version>
			<scope>test</scope>
		</dependency>
		<dependency>
	        <groupId>log4j</groupId>
	        <artifactId>log4j</artifactId>
	        <version>1.2.12</version>
	    </dependency>
		<dependency>
	        <groupId>no.nav.maven.plugins</groupId>
	        <artifactId>datapower-utils</artifactId>
	        <version>1.1.10</version>
	    </dependency>
		<dependency>
	        <groupId>no.nav.maven.plugins</groupId>
	        <artifactId>datapower-cfg-gen</artifactId>
	        <version>1.1.10</version>
	    </dependency>
	</dependencies>
	
	<distributionManagement>
		<repository>
			<id>nav-maven2-internal-repo</id>
			<name>NAV Maven2 Internal Repository</name>
			<url>scp://maven.adeo.no/var/maven2/internal</url>
		</repository>
		<snapshotRepository>
			<id>nav-pensjon-maven2-snapshot-repo</id>
			<name>NAV Maven2 Internal Snapshot Repository</name>
			<url>scp://maven.adeo.no/var/maven2/snapshot</url>
		</snapshotRepository>
	</distributionManagement>
</project>
