<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>nav.int.appclient.common</groupId>
	<artifactId>nav-int-appclient-common-java</artifactId>
	<version>0.2-SNAPSHOT</version>
	<packaging>jar</packaging>
	<name>Appclient common library</name>

	<scm>
		<connection>scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/incubator/release-test/nav-int-appclient-common-java</connection>
	</scm>

	<build>
		<plugins>
			<plugin>
				<artifactId>maven-release-plugin</artifactId>
				<configuration>
					<tagBase>https://versjonskontroll.adeo.no/svn/stelvio/tags/incubator/release-test</tagBase>
				</configuration>
			</plugin>
		</plugins>	
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-compiler-plugin</artifactId>
					<configuration>
						<source>1.5</source>
						<target>1.5</target>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>

	<dependencies>
		<dependency>
			<groupId>org.slf4j</groupId>
			<artifactId>slf4j-api</artifactId>
			<version>1.5.2</version>
		</dependency>
		<dependency>
			<groupId>commons-lang</groupId>
			<artifactId>commons-lang</artifactId>
			<version>2.1</version>
		</dependency>
		<dependency>
			<groupId>javax.ejb</groupId>
			<artifactId>ejb</artifactId>
			<version>2.1</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>com.ibm.wps</groupId>
			<artifactId>bfmclientmodel</artifactId>
			<version>6.1.0</version>
		</dependency>
		<dependency>
			<groupId>com.ibm.wps</groupId>
			<artifactId>bpcclientcore</artifactId>
			<version>6.1.0</version>
		</dependency>
		<dependency>
			<groupId>com.ibm.wps</groupId>
			<artifactId>bpeapi</artifactId>
			<version>6.1.0</version>
		</dependency>
		<dependency>
			<groupId>com.ibm.wps</groupId>
			<artifactId>bpe137650</artifactId>
			<version>6.1.0</version>
			<scope>runtime</scope>
		</dependency>
		<dependency>
			<groupId>com.ibm.wps</groupId>
			<artifactId>htmlclientmodel</artifactId>
			<version>6.1.0.1</version>
			<scope>runtime</scope>
		</dependency>
		<dependency>
			<groupId>com.ibm.ws</groupId>
			<artifactId>admin-client</artifactId>
			<version>6.1.0.7</version>
			<scope>provided</scope>
		</dependency>
	</dependencies>
	<distributionManagement>
		<repository>
			<id>nav-maven2-internal-repo</id>
			<url>scp://maven.adeo.no/var/maven2/internal</url>
		</repository>		
	</distributionManagement>
</project>