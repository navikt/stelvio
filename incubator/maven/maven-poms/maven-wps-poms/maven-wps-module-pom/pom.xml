<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>no.nav.maven.poms</groupId>
		<artifactId>maven-wps-root-pom</artifactId>
		<version>1.1-SNAPSHOT</version>
	</parent>

	<artifactId>maven-wps-module-pom</artifactId>
	<version>1.1-SNAPSHOT</version>
	<packaging>pom</packaging>
	<name>WPS Module Master POM</name>

	<build>
		<plugins>
			<!-- 
				TODO: This is an experimental plugin that currently (20/1-2009) only updates the display-name element 
				to include the Maven version.
			-->
			<plugin>
				<groupId>no.nav.maven.plugins</groupId>
				<artifactId>maven-update-ear-plugin</artifactId>
				<executions>
					<execution>
						<goals>
							<goal>update-ear</goal>
						</goals>
						<phase>package</phase>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
</project>