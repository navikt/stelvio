<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>no.nav.maven.poms</groupId>
		<artifactId>maven-sca-module-pom</artifactId>
		<version>1.0-SNAPSHOT</version>
	</parent>

	<groupId>no.nav.maven.poc</groupId>
	<artifactId>poc-not-working-module</artifactId>
	<packaging>sca-module-ear</packaging>
	<version>1.0-SNAPSHOT</version>

	<dependencies>
		<dependency>
			<groupId>no.nav.maven.poc</groupId>
			<artifactId>poc-lib</artifactId>
			<version>1.0-SNAPSHOT</version>
			<type>sca-library-jar</type>
		</dependency>
		<dependency>
			<groupId>no.nav.maven.poc</groupId>
			<artifactId>poc-lib2</artifactId>
			<version>1.0-SNAPSHOT</version>
			<type>sca-library-jar</type>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>no.nav.maven.plugins</groupId>
				<artifactId>maven-sca-artifact-plugin</artifactId>
				<extensions>true</extensions>
			</plugin>
			<plugin>
				<artifactId>maven-jar-plugin</artifactId>
				<configuration>
					<finalName>${artifactId}</finalName>
					<archive>
						<manifest>
							<mainClass>no.nav.java.Main</mainClass>
						</manifest>
						<manifestEntries>
							<Class-Path>
								${manifest.classpath}
							</Class-Path>
						</manifestEntries>
					</archive>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>