	xmlns:bons0="http://www.stelvio.no/example/order/V1">
	<xsd:include schemaLocation="DiscountType.xsd"></xsd:include>
	<xsd:complexType name="OrderLineItemSpecification">
		<xsd:sequence>
			<xsd:element minOccurs="1" name="productId"
