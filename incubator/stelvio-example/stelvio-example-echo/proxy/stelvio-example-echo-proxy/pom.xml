<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>no.stelvio.maven.poms</groupId>
		<artifactId>maven-wps-module-pom</artifactId>
		<version>2.0.1</version>
	</parent>

	<groupId>no.stelvio.example</groupId>
	<artifactId>stelvio-example-echo-proxy</artifactId>
	<version>1.0-SNAPSHOT</version>
	<packaging>wps-module-ear</packaging>
	<name>Stelvio Example ${project.artifactId}</name>
	
	<scm>
		<connection>${scm.connection}</connection>
		<developerConnection>${scm.connection}</developerConnection>
	</scm>
	
	<build>
		<extensions>
			<extension>
				<groupId>no.nav.maven.lifecycles</groupId>
				<artifactId>service-specification-lifecycle</artifactId>
				<version>1.0.0-SNAPSHOT</version>
			</extension>
		</extensions>
	</build>

	<dependencies>
		<dependency>
			<groupId>no.stelvio.example</groupId>
			<artifactId>stelvio-example-echo-service-specification</artifactId>
			<type>service-specification</type>
			<version>1.0.0-SNAPSHOT</version>
		</dependency>
	</dependencies>

	<properties>
		<scm.connection>${scm.root}</scm.connection>
		<scm.root>scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/incubator/stelvio-example/stelvio-example-echo/stelvio-example-echo-proxy</scm.root>
	</properties>
</project>
