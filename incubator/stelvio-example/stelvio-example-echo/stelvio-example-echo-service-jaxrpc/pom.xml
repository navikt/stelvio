<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<parent>
		<artifactId>maven-root-pom</artifactId>
		<groupId>no.stelvio.maven.poms</groupId>
		<version>1.2.2</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>

	<groupId>no.stelvio.example</groupId>
	<artifactId>stelvio-example-echo-service-jaxrpc</artifactId>
	<version>1.0-SNAPSHOT</version>
	<name>Stelvio Example Echo JAX-RPC interface</name>

	<build>
		<extensions>
			<extension>
				<groupId>no.nav.maven.lifecycles</groupId>
				<artifactId>service-specification-lifecycle</artifactId>
				<version>1.0.0-SNAPSHOT</version>
			</extension>
		</extensions>

		<plugins>
			<plugin>
				<groupId>no.nav.maven.plugins</groupId>
				<artifactId>maven-wsdl-plugin</artifactId>
				<version>1.12-SNAPSHOT</version>
				<executions>
					<execution>
						<goals>
							<goal>wsdl2java</goal>
						</goals>
						<configuration>
							<wsdlOptions>
								<wsdlOption>
									<wsdlArtifact>
										<groupId>no.stelvio.example</groupId>
										<artifactId>stelvio-example-echo-service-specification</artifactId>
										<version>1.0.0-SNAPSHOT</version>
										<type>service-specification</type>
									</wsdlArtifact>
								</wsdlOption>
							</wsdlOptions>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
</project>
