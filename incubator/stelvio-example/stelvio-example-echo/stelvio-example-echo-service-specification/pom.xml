<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<parent>
		<groupId>no.stelvio.maven.poms</groupId>
		<artifactId>maven-service-specification-pom</artifactId>
		<version>1.0.0-SNAPSHOT</version>
	</parent>

	<modelVersion>4.0.0</modelVersion>
	<groupId>no.stelvio.example</groupId>
	<artifactId>stelvio-example-echo-service-specification</artifactId>
	<version>1.0.0-SNAPSHOT</version>
	<packaging>service-specification</packaging>

	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.jvnet.jax-ws-commons</groupId>
					<artifactId>jaxws-maven-plugin</artifactId>
					<configuration>
						<wsdlFiles>
							<wsdlFile>example/services/V1/EchoService/ExampleProvider.wsdl</wsdlFile>
						</wsdlFiles>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>

	<properties>
		<endpointAddress>http://localhost:9080/stelvio-example-echo-proxyWeb/sca/EchoWSEXP</endpointAddress>
	</properties>

</project>