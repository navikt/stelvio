<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>no.stelvio.maven.poms</groupId>
		<artifactId>maven-wps-module-pom</artifactId>
		<version>2.0.1</version>
	</parent>

	<groupId>no.stelvio.example</groupId>
	<artifactId>stelvio-example-echo-service</artifactId>
	<version>1.0-SNAPSHOT</version>
	<packaging>wps-module-ear</packaging>
	<name>Stelvio Example ${project.artifactId}</name>

	<build>
		<plugins>
			<plugin>
				<groupId>no.nav.maven.plugins</groupId>
				<artifactId>maven-enforcer-plugin</artifactId>
				<version>1.1-SNAPSHOT</version>
				<executions>
					<execution>
						<id>jaxWsValidations</id>
						<configuration>
							<rules>
								<requirePolicySetRule
									implementation="no.stelvio.maven.enforcer.RequireScaSecurityPermission" />
							</rules>
						</configuration>
						<goals>
							<goal>enforce</goal>
						</goals>
						<phase>test</phase>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>

	<scm>
		<connection>${scm.connection}</connection>
		<developerConnection>${scm.connection}</developerConnection>
	</scm>

	<properties>
		<scm.connection>${scm.root}</scm.connection>
		<scm.root>scm:svn:https://versjonskontroll.adeo.no/svn/stelvio/trunk/incubator/stelvio-example/stelvio-example-echo/stelvio-example-echo-service</scm.root>
	</properties>
</project>