<mapping>
	<description>Basic mapping example</description>
	<class name="no.trygdeetaten.integration.framework.jms.formatter.OppdragTestBean" auto-complete="true">
		<map-to xml="oppdrag" />
		<field name="testField">
			<bind-xml name="field1"/>
		</field>
		<field name="d" type="double">
			<bind-xml name="doubleValue" node="element"/>
		</field>
		<field name="intTest" type="integer">
			<bind-xml name="integerValue" node="element"/>
		</field>
		<field name="elements" collection="collection" type="no.trygdeetaten.integration.framework.jms.formatter.TestBeanElement">
			<bind-xml name="element" node="element"/>
		</field> 
	</class>
	<class name="no.trygdeetaten.integration.framework.jms.formatter.OppdragTestBeanElement" auto-complete="true">
		<field name="val" type="string">
			<bind-xml name="fieldU" node="element"/>
		</field>
		<field name="dato" type="date" handler="no.trygdeetaten.integration.framework.jms.formatter.OppdragDateFormatter">
			<bind-xml name="fomDato" node="element"/>
		</field>
	</class>
</mapping>